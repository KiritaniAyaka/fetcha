{
	"compilerOptions": {
		"target": "ES6",
		"lib": ["ESNext"],
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"@/*": [
				"./src/*"
			]
		},
		"resolveJsonModule": true,
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true
	}
}
